	<!--content-->
	<div class="content">
		<div class="cart-items">
			<div class="container">
				<h2>Mi Carrito de Compras (<%= cart.totalQty %>)</h2>
				<% products = cart.items %>
				<% Object.keys(products).forEach(function(key){
				var product = products[key].item 
				var qty = products[key].qty %>
				<div class="cart-header">
					<div class="close1"> </div>
					<div class="cart-sec simpleCart_shelfItem">
						<div class="cart-item cyc">
							<img  src=<%= "../" + product.frontImage %> class="img-responsive" alt="">
						</div>
						<div class="cart-item-info">
							<h3><a href="#"> <%= product.code + " " + product.name %> </a><span>Pickup time:</span></h3>
							<p>Cantidad: <%= qty %></p>
							<ul class="qty">
								<li><p>Min. order value:</p></li>
								<li><p>FREE delivery</p></li>
							</ul>
							<div class="delivery">
